setup_dir <- function (x) {
if (is.null(x)) print("Please enter the path/to/your/directory")
if (!is.character(x)) x <- as.character(x)
if (.Platform$OS.type == "unix") {
setwd( paste0("~/", x) )
} else {
setwd( paste0("D:/", x) )
}
dir()
}
